#: src/notifier.cpp:22
#, kde-format
msgid "Upgrade available"
msgstr "Ka përmirësim të ri"

#: src/notifier.cpp:23
#, kde-format
msgctxt "For example, 'KDE neon 24.04 is available.'"
msgid "%1 %2 is available."
msgstr "Doli %1 %2."

#: src/notifier.cpp:25
#, kde-format
msgid ""
"\n"
"Your device will stop receiving updates and security fixes in 1 day — please "
"upgrade when possible."
msgid_plural ""
"\n"
"Your device will stop receiving updates and security fixes in %1 days — "
"please upgrade when possible."
msgstr[0] ""
"\n"
"Pajisja juaj do të ndalojë së marri përditësime dhe përmirësime sigurie në 1 "
"ditë — ju lutemi, përditësime kur të jetë e mundur."
msgstr[1] ""
"\n"
"Pajisja juaj do të ndalojë së marri përditësime dhe përmirësime sigurie në "
"%1 ditë — ju lutemi, përditësime kur të jetë e mundur."

#: src/notifier.cpp:30
#, kde-format
msgid ""
"\n"
"Your device is no longer receiving updates or security fixes — please "
"upgrade now."
msgstr ""
"\n"
"Pajisja juaj ka ndalur së marri përditësime dhe përmirësime sigurie në 1 "
"ditë — ju lutemi, përditësime tani."

#: src/notifier.cpp:49
#, kde-format
msgctxt "@action:button"
msgid "Upgrade"
msgstr "Përmirësoni"

#: src/screensaverinhibitor.cpp:24
#, kde-format
msgctxt "Screensaver inhibition reason"
msgid "Upgrading Operating System"
msgstr "Duke Përmirësuar Sistemin Operativ"

#: src/upgraderprocess.cpp:98
#, kde-format
msgctxt "@title/window upgrade failure dialog"
msgid "Upgrade Failed"
msgstr "Përmirësimi Dështoi"

