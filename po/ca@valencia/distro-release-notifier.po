#: src/notifier.cpp:22
#, kde-format
msgid "Upgrade available"
msgstr "Hi ha una actualització disponible"

#: src/notifier.cpp:23
#, kde-format
msgctxt "For example, 'KDE neon 18.04 is available.'"
msgid "%1 %2 is available."
msgstr "%1 %2 està disponible."

#: src/notifier.cpp:25
#, kde-format
msgid ""
"\n"
"Your device will stop receiving updates and security fixes in 1 day - please "
"upgrade when possible."
msgid_plural ""
"\n"
"Your device will stop receiving updates and security fixes in %1 days - "
"please upgrade when possible."
msgstr[0] ""
"\n"
"Este dispositiu deixarà de rebre actualitzacions i correccions de seguretat "
"en 1 dia. Actualitzeu-lo quan siga possible."
msgstr[1] ""
"\n"
"Este dispositiu deixarà de rebre actualitzacions i correccions de seguretat "
"en %1 dies. Actualitzeu-lo quan siga possible."

#: src/notifier.cpp:30
#, kde-format
msgid ""
"\n"
"Your device is no longer receiving updates or security fixes - please "
"upgrade now."
msgstr ""
"\n"
"Esta dispositiu ja no rebrà actualitzacions o correccions de seguretat. "
"Actualitzeu-lo ara."

#: src/screensaverinhibitor.cpp:19
#, kde-format
msgctxt "Screensaver inhibition reason"
msgid "Upgrading Operating System"
msgstr "Actualització del sistema operatiu"

#: src/upgraderprocess.cpp:98
#, kde-format
msgctxt "@title/window upgrade failure dialog"
msgid "Upgrade Failed"
msgstr "No s'ha pogut fer l'actualització"

#: src/upgraderprocess.cpp:104
#, kde-format
msgctxt "@title title widget above process output"
msgid "Upgrade failed with the following output:"
msgstr "No s'ha pogut fer l'actualització amb l'eixida següent:"
